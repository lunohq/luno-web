{
  "name": "luno-web",
  "version": "1.0.2",
  "description": "Luno web app",
  "scripts": {
    "start": "npm run update && babel-node server/index.js",
    "dev": "npm run update && ./bin/server",
    "update": "babel-node server/utils/updateSchema.js",
    "deploy": "eval \"$(docker-machine env default)\" && npm unlink luno-core && NODE_ENV=production npm install && NODE_ENV=production webpack --config webpack.production.config.js && npm run docker",
    "lint": "eslint --ignore-path .gitignore client server",
    "docker-build": "docker build -t lunohq/luno-web . && docker tag lunohq/luno-web lunohq/luno-web:`git log -n 1 --pretty=format:'%H'`",
    "docker-push": "docker push lunohq/luno-web",
    "docker": "npm run docker-build && npm run docker-push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lunohq/luno-web.git"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.8.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "gaze": "^1.0.0",
    "html-webpack-plugin": "^2.15.0",
    "isomorphic-style-loader": "^1.0.0",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.21",
    "postcss-import": "^8.1.1",
    "postcss-loader": "^0.9.1",
    "postcss-scss": "^0.1.7",
    "precss": "^1.4.0",
    "raven-js": "^2.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.2.0",
