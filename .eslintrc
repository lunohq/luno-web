{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "guard-for-in": 0,
    "spaced-comment": 0,
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "jsx-quotes": [2, "prefer-single"],
    "no-param-reassign": 0,
    "no-shadow": 0,
    "react/sort-comp": 1,
    "react/sort-prop-types": 1,
    "no-confusing-arrow": 0,
    "semi": 0,
    "new-cap": 0
  }
}
